{
  "name": "n86io/rest",
  "description": "Currently not standalone working php rest-library.",
  "license": "GPL-3.0+",
  "keywords": [
    "rest",
    "api"
  ],
  "authors": [
    {
      "name": "Viktor Firus",
      "email": "v@n86.io"
    }
  ],
  "require": {
    "php": ">= 5.5",
    "ext-json": "*",
    "doctrine/annotations": "~1.2.0",
    "doctrine/cache": "^1.6",
    "lcobucci/jwt": "^3.2",
    "n86io/reflection": "^0.1",
    "php-di/php-di": "^5.4",
    "psr/http-message": "^1.0",
    "symfony/yaml": "^3.1",
    "willdurand/negotiation": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "guzzlehttp/psr7": "^1.3",
    "mockery/mockery": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "N86io\\Rest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "N86io\\Rest\\Tests\\": "tests/Unit"
    }
  },
  "suggest": {
    "guzzlehttp/psr7": "If no implementation for psr/http-message available, this package or other require with implementation for psr/http-message."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}