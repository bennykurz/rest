{
  "name": "n86io/rest",
  "description": "Currently not standalone working php rest-library.",
  "license": "GPL-3.0+",
  "keywords": [
    "rest",
    "api"
  ],
  "authors": [
    {
      "name": "Viktor Firus",
      "email": "v@n86.io"
    }
  ],
  "require": {
    "php": ">= 5.5",
    "ext-json": "*",
    "doctrine/cache": "^1.6",
    "lcobucci/jwt": "^3.2",
    "n86io/reflection": "^0.1",
    "psr/http-message": "^1.0",
    "webmozart/assert": "^1.1",
    "willdurand/negotiation": "^2.2"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "mikey179/vfsStream": "^1.6",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "~4.8.0",
    "symfony/yaml": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "N86io\\Rest\\": "src",
      "N86io\\Rest\\Examples\\": "examples"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "N86io\\Rest\\Tests\\Unit\\": "build/tests/unit"
    }
  },
  "suggest": {
    "guzzlehttp/psr7": "If no implementation for psr/http-message available, this package or other require with implementation for psr/http-message.",
    "symfony/yaml": "Required, if EntityInfo configurations in yaml exist."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "scripts": {
    "test": [
      "@composer install",
      "./vendor/bin/phpunit -c build/tests/unit.xml --coverage-clover build/logs/clover.xml"
    ],
    "test-report": [
      "@test",
      "./vendor/bin/test-reporter"
    ]
  }
}